#!/bin/zsh

brewinstall zsh-completions

brewcaskinstall "iTerm" "iterm2" $HOME/Applications
-dockutil --no-restart --add "~/Applications/iTerm.app" --before "Console"

source $DOTFILES/components/_cli/preferences/iTerm2

# better than man
npmig tldr

# safer than rm
npmig trash

# essential missing commands to OSX
brewinstall tree
brewinstall ssh-copy-id

# to create favicons
brewinstall icoutils

brewtap sitespeedio/sitespeedio
brewtap tobli/browsertime
brewinstall sitespeed.io

#sudo pip install --upgrade asciinema
#asciinema auth
#asciinema rec
